{
  "name": "@svton/nestjs-logger",
  "version": "1.3.0",
  "description": "NestJS logger module with pino and request tracing",
  "keywords": [
    "nestjs",
    "logger",
    "pino",
    "trace",
    "request-id"
  ],
  "author": "SVTON Team",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "dependencies": {
    "nestjs-pino": "^4.0.0",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@types/node": "^20.10.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
