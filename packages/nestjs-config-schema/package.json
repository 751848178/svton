{
  "name": "@svton/nestjs-config-schema",
  "version": "1.2.0",
  "description": "NestJS config validation with Zod/Joi schema support",
  "keywords": [
    "nestjs",
    "config",
    "validation",
    "zod",
    "joi",
    "env"
  ],
  "author": "SVTON Team",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@nestjs/config": "^3.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/config": "^3.1.1",
    "@types/node": "^20.10.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
