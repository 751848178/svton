{
  "name": "@svton/nestjs-cache",
  "version": "1.0.0",
  "description": "NestJS cache decorators with Redis support (@Cacheable, @CacheEvict, @CachePut)",
  "keywords": [
    "nestjs",
    "cache",
    "redis",
    "decorator",
    "cacheable"
  ],
  "author": "SVTON Team",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@svton/nestjs-redis": "^1.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@svton/nestjs-redis": "workspace:*",
    "@types/node": "^20.10.0",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
