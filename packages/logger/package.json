{
  "name": "@svton/logger",
  "version": "0.1.0",
  "description": "Frontend logging and error tracking with plugin support",
  "keywords": [
    "logger",
    "error-tracking",
    "performance",
    "monitoring",
    "frontend"
  ],
  "author": "SVTON Team",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
