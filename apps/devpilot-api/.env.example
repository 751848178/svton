# Application
NODE_ENV=development
PORT=3101
CORS_ORIGIN=http://localhost:3100

# Logging
LOG_LEVEL=debug

# Database (MySQL)
DATABASE_URL="mysql://user:password@localhost:3306/devpilot"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Cache
CACHE_ENABLED=true
CACHE_TTL=300

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# OAuth - GitHub
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# OAuth - GitLab
GITLAB_CLIENT_ID=
GITLAB_CLIENT_SECRET=

# OAuth - Gitee
GITEE_CLIENT_ID=
GITEE_CLIENT_SECRET=

# Encryption (for resource credentials - must be 32 characters)
ENCRYPTION_KEY=your-32-character-encryption-key!
